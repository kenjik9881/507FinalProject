        <script>
            $( document ).ready(function() {
                // GTM Session Set
                var maxExpire = new Date('2038-01-19 00:00:00');
                if (Cookies.get("gtm_session_first") === undefined) {
                    Cookies.set("gtm_session_first", new Date(), {expires: maxExpire})
                }            

                Cookies.set("gtm_session_last", new Date(), {expires: maxExpire})
                
                // ICO Banner Prefs
                var wrapper = $('#icobanner-wrapper');
                
                if (Cookies.get("icobanner_noshow18") != "1") {
                    $('#icobanner-wrapper').show();
                }

                $(".icobanner-close").on('click', function () {
                    Cookies.set("icobanner_noshow18", "1", {expires: 1})
                    wrapper.fadeOut();
                    dataLayer.push({
                        'event': 'customEvent',
                        'eventCategory': 'Countdown Banner',
                        'eventAction': 'Close',
                        'eventLabel': 'vyral',
                    });
                });

            });

            $('[data-language-toggle]').click(function () {
                dataLayer.push({
                    'event': 'customEvent',
                    'eventCategory': 'Language Dropdown',
                    'eventAction': 'Language Selected',
                    'eventLabel': $(this).data('language-toggle')
                });
            });

            var language = $('[data-language-toggle="' + 'en' + '"]').html();
            $('[data-language-dropdown]').html(language);
        </script>

